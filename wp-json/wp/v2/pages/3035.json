u05d9 \u05e9\u05d0\u05ea\u05dd \u05d1\u05d5\u05d7\u05e8\u05d9\u05dd \u05d0\u05ea \u05d4\u05d0\u05e4\u05e9\u05e8\u05d5\u05ea \u05d4\u05e8\u05d0\u05e9\u05d5\u05e0\u05d4 \u05e9\u05d0\u05ea\u05dd \u05de\u05d5\u05e6\u05d0\u05d9\u05dd, \u05e9\u05d9\u05de\u05d5 \u05dc\u05d1 \u05d0\u05d9\u05da \u05d1\u05d5\u05d7\u05e8\u05d9\u05dd \u05d1\u05e2\u05dc \u05de\u05e7\u05e6\u05d5\u05e2 \u05d1\u05e6\u05d5\u05e8\u05d4 \u05e0\u05db\u05d5\u05e0\u05d4:<\/p><p><br \/><strong>\u05ea\u05e2\u05d5\u05d3\u05ea \u05d9\u05d5\u05e9\u05e8 &#8211;<\/strong> \u05e0\u05ea\u05d7\u05d9\u05dc \u05d3\u05d5\u05d5\u05e7\u05d0 \u05d1\u05e4\u05e8\u05d8 \u05e9\u05d0\u05de\u05e0\u05dd \u05d9\u05db\u05d5\u05dc \u05dc\u05d4\u05e8\u05d0\u05d5\u05ea \u05e9\u05d5\u05dc\u05d9 \u05dc\u05de\u05e1\u05e4\u05e8 \u05d0\u05e0\u05e9\u05d9\u05dd, \u05d0\u05d1\u05dc \u05d4\u05d5\u05d0 \u05de\u05e9\u05e7\u05e3 \u05d0\u05de\u05d9\u05e0\u05d5\u05ea \u05d5\u05de\u05e7\u05e6\u05d5\u05e2\u05d9\u05d5\u05ea \u05de\u05d8\u05e2\u05dd \u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2. \u05ea\u05e2\u05d5\u05d3\u05ea \u05d9\u05d5\u05e9\u05e8 \u05d0\u05d9\u05e0\u05d4 \u05d7\u05d5\u05d1\u05d4, \u05dc\u05de\u05e2\u05e9\u05d4 \u05d0\u05e4\u05d9\u05dc\u05d5 \u05d4\u05e1\u05de\u05db\u05d4 \u05e9\u05dc \u05e4\u05d5\u05e8\u05e5 \u05de\u05e0\u05e2\u05d5\u05dc\u05d9\u05dd \u05d0\u05d9\u05e0\u05d4 \u05d7\u05d5\u05d1\u05d4. \u05e2\u05dd \u05d6\u05d0\u05ea, \u05ea\u05e2\u05d5\u05d3\u05ea \u05d9\u05d5\u05e9\u05e8 \u05d1\u05d0\u05d4 \u05dc\u05d8\u05d5\u05d1\u05ea \u05d4\u05dc\u05e7\u05d5\u05d7, \u05d1\u05e9\u05d1\u05d9\u05dc \u05dc\u05d4\u05e8\u05d0\u05d5\u05ea \u05dc\u05d5 \u05e9\u05e0\u05d9\u05ea\u05df \u05dc\u05e1\u05de\u05d5\u05da \u05e2\u05dc \u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2. \u05ea\u05e2\u05d5\u05d3\u05ea \u05d9\u05d5\u05e9\u05e8 \u05d0\u05d9\u05e0\u05d4 \u05d7\u05d5\u05d1\u05d4 \u05d0\u05da \u05d4\u05d9\u05d0 \u05de\u05e9\u05e7\u05e4\u05ea \u05e9\u05d0\u05d9\u05df \u05dc\u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2 \u05e2\u05d1\u05e8 \u05e4\u05dc\u05d9\u05dc\u05d9 \u05d5\u05d4\u05d5\u05d0 \u05e2\u05d5\u05d1\u05d3 \u05e2\u05dd \u05d9\u05d3\u05d9\u05e2\u05ea \u05d4\u05de\u05e9\u05d8\u05e8\u05d4. \u05d1\u05e2\u05d1\u05e8 \u05dc\u05d0 \u05d4\u05d9\u05d4 \u05e0\u05d4\u05d5\u05d2 \u05dc\u05d4\u05d9\u05e2\u05d6\u05e8 \u05d1\u05ea\u05e2\u05d5\u05d3\u05ea \u05d9\u05d5\u05e9\u05e8, \u05d0\u05dd \u05d6\u05d0\u05ea \u05d4\u05e8\u05d1\u05d4 \u05e4\u05e2\u05de\u05d9\u05dd \u05e4\u05d5\u05e8\u05e6\u05d9\u05dd \u05d1\u05d4\u05e1\u05d5\u05d5\u05d0\u05d4 \u05e9\u05dc \u05de\u05e0\u05e2\u05d5\u05dc\u05d9\u05dd \u05d4\u05d9\u05d5 \u05de\u05e2\u05e0\u05d9\u05e7\u05d9\u05dd \u05e9\u05d9\u05e8\u05d5\u05ea \u05d5\u05d1\u05db\u05da \u05de\u05e0\u05e6\u05dc\u05d9\u05dd \u05d0\u05ea \u05d4\u05dc\u05e7\u05d5\u05d7, \u05de\u05d0\u05d6 \u05de\u05e9\u05d8\u05e8\u05ea \u05d9\u05e9\u05e8\u05d0\u05dc \u05de\u05e2\u05e0\u05d9\u05e7\u05d4 \u05dc\u05db\u05dc \u05d1\u05e2\u05dc \u05de\u05e7\u05e6\u05d5\u05e2 \u05d0\u05e4\u05e9\u05e8\u05d5\u05ea \u05dc\u05e7\u05d1\u05dc \u05ea\u05e2\u05d5\u05d3\u05ea \u05d9\u05d5\u05e9\u05e8 \u05d0\u05dd \u05d4\u05d5\u05d0 \u05e2\u05d5\u05e0\u05d4 \u05e2\u05dc \u05d4\u05ea\u05e0\u05d0\u05d9\u05dd \u05d4\u05db\u05ea\u05d5\u05d1\u05d9\u05dd \u05d1\u05d7\u05d5\u05e7.<\/p><p>\u00a0<\/p><p><strong>\u05e0\u05d9\u05e1\u05d9\u05d5\u05df &#8211;<\/strong> \u05e2\u05d3 \u05db\u05de\u05d4 \u05d4\u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05d7\u05e9\u05d5\u05d1? \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05de\u05e9\u05e7\u05e3 \u05dc\u05e0\u05d5 \u05de\u05e1\u05e4\u05e8 \u05d3\u05d1\u05e8\u05d9\u05dd: \u05de\u05e7\u05e6\u05d5\u05e2\u05d9\u05d5\u05ea, \u05d0\u05de\u05d9\u05e0\u05d5\u05ea \u05d5\u05d9\u05d5\u05e9\u05e8. \u05db\u05d0\u05e9\u05e8 \u05de\u05d3\u05d5\u05d1\u05e8 \u05e2\u05dc \u05e2\u05e1\u05e7 \u05e2\u05e6\u05de\u05d0\u05d9 \u05d0\u05d9\u05df \u05e4\u05d9\u05e7\u05d5\u05d7 \u05e2\u05dc \u05d4\u05de\u05d7\u05d9\u05e8, \u05db\u05dc \u05d1\u05e2\u05dc \u05de\u05e7\u05e6\u05d5\u05e2 \u05d9\u05db\u05d5\u05dc \u05dc\u05d3\u05e8\u05d5\u05e9 \u05de\u05db\u05dd \u05db\u05dc \u05e1\u05db\u05d5\u05dd \u05d0\u05d5\u05ea\u05d5 \u05d9\u05e8\u05e6\u05d4. \u05e2\u05dd \u05d6\u05d0\u05ea, \u05d0\u05e0\u05e9\u05d9\u05dd \u05e9\u05e6\u05d1\u05e8\u05d5 \u05d3\u05d9 \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05de\u05d1\u05d9\u05e0\u05d9\u05dd \u05e9\u05de\u05d4 \u05e9\u05d7\u05e9\u05d5\u05d1 \u05d4\u05d5\u05d0 \u05dc\u05d0 \u05d4\u05e2\u05e1\u05e7\u05d4 \u05d4\u05e1\u05e4\u05e6\u05d9\u05e4\u05d9\u05ea \u05d4\u05d6\u05d0\u05ea, \u05d0\u05dc\u05d0 \u05dc\u05e9\u05de\u05e8 \u05dc\u05e7\u05d5\u05d7\u05d5\u05ea. \u05d4\u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05d2\u05dd \u05de\u05e2\u05e0\u05d9\u05e7 \u05dc\u05db\u05dd \u05d4\u05dc\u05e7\u05d5\u05d7\u05d5\u05ea \u05d9\u05ea\u05e8\u05d5\u05df, \u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2 \u05de\u05d1\u05e6\u05e2 \u05d0\u05ea \u05d4\u05de\u05dc\u05d0\u05db\u05d4 \u05d1\u05e6\u05d5\u05e8\u05d4 \u05d9\u05e2\u05d9\u05dc\u05d4 \u05d5\u05de\u05d4\u05d9\u05e8\u05d4. \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05de\u05d0\u05e4\u05e9\u05e8 \u05dc\u05db\u05dd \u05dc\u05d4\u05e0\u05d5\u05ea \u05de\u05e4\u05e8\u05d9\u05e6\u05d4 \u05e9\u05e7\u05d8\u05d4. \u05e4\u05e8\u05d9\u05e6\u05d4 \u05e9\u05e7\u05d8\u05d4 \u05dc\u05e2\u05d9\u05ea\u05d9\u05dd \u05d9\u05db\u05d5\u05dc\u05d4 \u05dc\u05d4\u05d2\u05d9\u05e2 \u05d1\u05e7\u05d5\u05e0\u05d5\u05d8\u05e6\u05d9\u05d4 \u05e9\u05dc\u05d9\u05dc\u05d9\u05ea \u05db\u05d0\u05e9\u05e8 \u05d4\u05ea\u05e7\u05d9\u05d9\u05de\u05d4 \u05e4\u05e8\u05d9\u05e6\u05d4 \u05d5\u05dc\u05d0 \u05e9\u05de\u05e0\u05d5 \u05dc\u05d1 \u05dc\u05e9\u05d5\u05dd \u05e1\u05d9\u05de\u05df \u05d4\u05de\u05e2\u05d9\u05d3 \u05e2\u05dc \u05db\u05da. \u05e2\u05dd \u05d6\u05d0\u05ea, \u05db\u05d0\u05e9\u05e8 \u05d0\u05ea\u05dd \u05de\u05d6\u05de\u05d9\u05e0\u05d9\u05dd \u05d1\u05e2\u05dc \u05de\u05e7\u05e6\u05d5\u05e2 \u05d4\u05de\u05d1\u05e6\u05e2 \u05e4\u05e8\u05d9\u05e6\u05d4 \u05e9\u05e7\u05d8\u05d4 \u05d0\u05ea\u05dd \u05dc\u05de\u05e2\u05e9\u05d4 \u05e0\u05d4\u05e0\u05d9\u05dd \u05de\u05db\u05de\u05d5\u05ea \u05d1\u05dc\u05d0\u05d9 \u05e0\u05de\u05d5\u05db\u05d4 \u05d0\u05dd \u05d1\u05db\u05dc\u05dc. \u05de\u05e6\u05d3 \u05e9\u05e0\u05d9, \u05db\u05d0\u05e9\u05e8 \u05ea\u05d6\u05de\u05d9\u05e0\u05d5 \u05e4\u05d5\u05e8\u05e5 \u05de\u05e0\u05e2\u05d5\u05dc\u05d9\u05dd \u05dc\u05d0 \u05de\u05e0\u05d5\u05e1\u05d4 \u05d4\u05d5\u05d0 \u05e2\u05e9\u05d5\u05d9 \u05dc\u05e4\u05d2\u05d5\u05e2 \u05d1\u05d3\u05dc\u05ea \u05e2\u05e6\u05de\u05d4 \u05d0\u05d5 \u05d1\u05de\u05e0\u05d2\u05e0\u05d5\u05df \u05d4\u05e0\u05e2\u05d9\u05dc\u05d4 \u05ea\u05d5\u05da \u05db\u05d3\u05d9 \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05dc\u05e4\u05ea\u05d5\u05e8 \u05d0\u05ea \u05d4\u05d1\u05e2\u05d9\u05d4.<\/p><p>\u00a0<\/p><p><strong>\u05e9\u05e7\u05d9\u05e4\u05d5\u05ea &#8211;<\/strong> \u05d1\u05e2\u05d9\u05e0\u05d9\u05e0\u05d5 \u05e9\u05e7\u05d9\u05e4\u05d5\u05ea \u05d4\u05d9\u05d0 \u05d4\u05d3\u05d1\u05e8 \u05d4\u05d7\u05e9\u05d5\u05d1 \u05d1\u05d9\u05d5\u05ea\u05e8 \u05d1\u05de\u05e7\u05e6\u05d5\u05e2. \u05de\u05db\u05d9\u05d5\u05d5\u05df \u05e9\u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2 \u05de\u05d1\u05e6\u05e2 \u05d0\u05ea \u05d4\u05de\u05dc\u05d0\u05db\u05d4 \u05d5\u05d4\u05dc\u05e7\u05d5\u05d7 \u05dc\u05d0 \u05d1\u05d0\u05de\u05ea \u05de\u05d1\u05d9\u05df \u05de\u05d4 \u05e7\u05d5\u05e8\u05d4, \u05d7\u05e9\u05d5\u05d1 \u05dc\u05e2\u05d3\u05db\u05df \u05d0\u05ea \u05d4\u05dc\u05e7\u05d5\u05d7 \u05d1\u05ea\u05d4\u05dc\u05d9\u05da \u05d1\u05db\u05d3\u05d9 \u05e9\u05d9\u05d1\u05d9\u05df \u05e2\u05dc \u05de\u05d4 \u05d4\u05d5\u05d0 \u05de\u05e9\u05dc\u05dd \u05d1\u05d3\u05d9\u05d5\u05e7. \u05d4\u05e9\u05e7\u05d9\u05e4\u05d5\u05ea \u05de\u05d0\u05e4\u05e9\u05e8\u05ea \u05dc\u05db\u05dd \u05dc\u05e1\u05de\u05d5\u05da \u05e2\u05dc \u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2, \u05dc\u05d3\u05e2\u05ea \u05de\u05d4 \u05d4\u05d9\u05d9\u05ea\u05d4 \u05d4\u05d1\u05e2\u05d9\u05d4 \u05d5\u05db\u05de\u05d5 \u05db\u05df, \u05db\u05d9\u05e6\u05d3 \u05ea\u05d5\u05db\u05dc\u05d5 \u05dc\u05d4\u05d9\u05de\u05e0\u05e2 \u05de\u05d0\u05d5\u05ea\u05d4 \u05ea\u05e7\u05dc\u05d4 \u05d1\u05e9\u05e0\u05d9\u05ea. \u05d1\u05e8\u05d5\u05d1 \u05d4\u05de\u05e7\u05e8\u05d9\u05dd \u05ea\u05e7\u05dc\u05d5\u05ea \u05de\u05ea\u05e8\u05d7\u05e9\u05d5\u05ea \u05e2\u05e7\u05d1 \u05d4\u05ea\u05e0\u05d4\u05dc\u05d5\u05ea \u05dc\u05e7\u05d5\u05d9\u05d4 \u05e9\u05dc\u05d0 \u05d1\u05d9\u05d3\u05d9\u05e2\u05ea\u05d5 \u05e9\u05dc \u05d4\u05dc\u05e7\u05d5\u05d7.<\/p>\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-336d6a3 elementor-widget elementor-widget-heading\" data-id=\"336d6a3\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t<h3 class=\"elementor-heading-title elementor-size-default\">\u05de\u05d4\u05d9 \u05e4\u05e8\u05d9\u05e6\u05ea \u05de\u05e0\u05e2\u05d5\u05dc\u05d9\u05dd \u05dc\u05e8\u05db\u05d1 \u05d1\u05e0\u05e1 \u05e6\u05d9\u05d5\u05e0\u05d4?<\/h3>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-0fda63b elementor-widget elementor-widget-text-editor\" data-id=\"0fda63b\" data-element_type=\"widget\" data-widget_type=\"text-editor.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\u05dc\u05d0 \u05db\u05dc \u05e4\u05d5\u05e8\u05e5 \u05de\u05e0\u05e2\u05d5\u05dc\u05d9\u05dd \u05db\u05e9\u05d9\u05e8 \u05dc\u05e4\u05e8\u05d5\u05e5 \u05de\u05e0\u05e2\u05d5\u05dc \u05dc\u05e8\u05db\u05d1, \u05e4\u05e8\u05d9\u05e6\u05ea \u05de\u05e0\u05e2\u05d5\u05dc\u05d9 \u05e8\u05db\u05d1 \u05de\u05e6\u05e8\u05d9\u05db\u05d4 \u05de\u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2 \u05dc\u05d4\u05d7\u05d6\u05d9\u05e7 \u05d1\u05d9\u05d3\u05e2 \u05d4\u05e0\u05d3\u05e8\u05e9 \u05d1\u05d4\u05ea\u05d0\u05dd \u05dc\u05e1\u05d5\u05d2 \u05d4\u05e8\u05db\u05d1. \u05dc\u05db\u05dc \u05e1\u05d5\u05d2 \u05e8\u05db\u05d1 \u05d9\u05e9 \u05d0\u05ea \u05d3\u05e8\u05db\u05d9 \u05d4\u05e4\u05e8\u05d9\u05e6\u05d4 \u05e9\u05dc\u05d5 \u05d5\u05d0\u05d9 \u05d9\u05d3\u05d9\u05e2\u05d4 \u05d0\u05d5 \u05d7\u05d5\u05e1\u05e8 \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05d9\u05db\u05d5\u05dc\u05d9\u05dd \u05dc\u05d4\u05d5\u05d1\u05d9\u05dc \u05dc\u05ea\u05e7\u05dc\u05d4 \u05de\u05d5\u05e8\u05db\u05d1\u05ea \u05d4\u05e8\u05d1\u05d4 \u05d9\u05d5\u05ea\u05e8, \u05d5\u05d1\u05db\u05da \u05d4\u05e2\u05dc\u05d5\u05ea \u05e9\u05dc \u05d4\u05e9\u05d9\u05e8\u05d5\u05ea \u05ea\u05ea\u05d9\u05d9\u05e7\u05e8. \u05db\u05d0\u05e9\u05e8 \u05d9\u05e9 \u05dc\u05db\u05dd \u05ea\u05e7\u05dc\u05d4 \u05d1\u05e8\u05db\u05d1, \u05d1\u05d9\u05df \u05d0\u05dd \u05de\u05d3\u05d5\u05d1\u05e8 \u05e2\u05dc \u05e1\u05d5\u05d5\u05d9\u05e5' \u05ea\u05e7\u05d5\u05e2, \u05d0\u05d5\u05d1\u05d3\u05df \u05de\u05e4\u05ea\u05d7, \u05e7\u05d5\u05d3\u05df \u05dc\u05d0 \u05de\u05ea\u05e4\u05e7\u05d3 \u05d0\u05d5 \u05db\u05dc \u05ea\u05e7\u05dc\u05d4 \u05d0\u05d7\u05e8\u05ea &#8211; \u05d4\u05d6\u05de\u05d9\u05e0\u05d5 \u05d1\u05e2\u05dc \u05de\u05e7\u05e6\u05d5\u05e2 \u05d0\u05e9\u05e8 \u05de\u05ea\u05de\u05d7\u05d4 \u05d1\u05e4\u05e8\u05d9\u05e6\u05ea \u05e8\u05db\u05d1\u05d9\u05dd. \u05d1\u05d3\u05d9\u05d5\u05e7 \u05db\u05de\u05d5 \u05d1\u05e9\u05d9\u05e8\u05d5\u05ea\u05d9 \u05de\u05e0\u05e2\u05d5\u05dc\u05e0\u05d5\u05ea \u05dc\u05d1\u05d9\u05ea, \u05d2\u05dd \u05db\u05d0\u05df \u05d1\u05e2\u05dc \u05d4\u05de\u05e7\u05e6\u05d5\u05e2 \u05e6\u05e8\u05d9\u05da \u05dc\u05d4\u05d2\u05d9\u05e2 \u05d0\u05dc\u05d9\u05db\u05dd \u05dc\u05e4\u05d9 \u05d6\u05de\u05df \u05d4\u05d4\u05ea\u05d7\u05d9\u05d9\u05d1\u05d5\u05ea \u05d4\u05de\u05e9\u05d5\u05e2\u05e8 \u05e9\u05e0\u05e2 \u05d1\u05d9\u05df 20 \u05d3\u05e7\u05d5\u05ea \u05dc\u05d7\u05e6\u05d9 \u05e9\u05e2\u05d4. \u05dc\u05e8\u05d5\u05d1 \u05d1\u05e2\u05dc\u05d9 \u05de\u05e7\u05e6\u05d5\u05e2 \u05d0\u05dc\u05d5 \u05d9\u05e0\u05d5\u05e2\u05d5 \u05d1\u05d0\u05de\u05e6\u05e2\u05d5\u05ea \u05e7\u05d8\u05e0\u0